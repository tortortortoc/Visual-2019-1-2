<Window x:Class="N019_SnakeBite.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:N019_SnakeBite"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Image x:Name="image" Margin="0,0,-0.4,0" Stretch="Fill" Source="pooh.jpg"/>
        <Button x:Name="btnStart" Content="Play" Margin="656,138,62.6,0"  Click="btnPlay_Click" Height="49" VerticalAlignment="Top" />
        <Button x:Name="btnQuit" Content="Quit" Margin="656,219,62.6,0" Click="btnQuit_Click" Height="47" VerticalAlignment="Top" />
        <TextBlock HorizontalAlignment="Left" Margin="26,22,0,0" TextWrapping="Wrap" Text="Eat 25 Eggs Using Arrow Keys" VerticalAlignment="Top"/>

    </Grid>
</Window>
